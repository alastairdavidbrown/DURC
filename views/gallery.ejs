<!DOCTYPE html>
<html lang="en">

<head>
	<title>Starter Template for Bootstrap</title>
	<% include head-frag %>


	<!--START carousel-->
	<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
	<script src="dist/js/bootstrap.min.js"></script>
	<!--END carousel-->

</head>

<body>
	
	<% include nav-bar-frag %>

	<div class="container">
		<div class="row">
			<div class="col-md-12">
				<div class="well">
					<h5>This is our gallery with pictures of the church</h5>
				</div>



				<div id="this-carousel-id" class="carousel slide" data-ride="carousel">
					<!-- class of slide for animation -->
					<div class="carousel-inner" role="listbox">
						<div class="item active">
							<img class="featurette-image img-rounded img-responsive"  src="<%= 'images/OurHistory/'+files[0] %>" alt="" />
						</div>

						<% for(var i=1; i<files.length; i++) { %>
							<% console.log (files[i]) %>
								<div class="item">
									<img class="featurette-image img-rounded img-responsive" src="<%= 'images/OurHistory/'+files[i] %>" alt="" />
								</div>
						<% } %>

					</div>
					<!-- Left and right controls -->
					<a class="left carousel-control" href="#this-carousel-id" role="button" data-slide="prev">
						<span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
						<span class="sr-only">Previous</span>
					</a>
					<a class="right carousel-control" href="#this-carousel-id" role="button" data-slide="next">
						<span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
						<span class="sr-only">Next</span>
					</a>
					<!-- /.carousel -->
				</div>
			</div>
		</div>
	</div>
	
	<% include bootstrap-frag %>

</body>